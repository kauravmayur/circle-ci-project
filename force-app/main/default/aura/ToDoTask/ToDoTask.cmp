      <td data-label="Task">
        <div class="slds-truncate" title="{!v.taskName}">
          <ui:outputText  value="{!v.taskName}"/>
        </div>
      </td>
      <td data-label="Action">
        <c:ToDoDeleteButton/>
      </td>
    </tr>
</aura:component>